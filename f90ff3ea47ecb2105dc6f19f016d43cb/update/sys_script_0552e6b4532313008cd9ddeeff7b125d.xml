        <collection>sys_atf_parameter_set</collection>
        <condition/>
        <description>When a parameter set is placed in between a group of parameter sets, all sets that come after the newly placed one must auto increment.</description>
        <execute_function>false</execute_function>
        <filter_condition/>
        <is_rest>false</is_rest>
        <message/>
        <name>Reorder test parameter sets order</name>
        <order>200</order>
        <priority>100</priority>
        <rest_method/>
        <rest_method_text/>
        <rest_service/>
        <rest_service_text/>
        <rest_variables/>
        <role_conditions/>
        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {
      var testId = current.getValue('test');
      var atfTest = new GlideRecord("sys_atf_test");

      // dont re-order param_set before a mass delete triggered by the parent sys_atf_test being deleted
      if (atfTest.get(testId)) {
        new ATFOrderUpdate().reorderAfterRemove('sys_atf_parameter_set', current);
      }
	
})(current, previous);]]></script>
