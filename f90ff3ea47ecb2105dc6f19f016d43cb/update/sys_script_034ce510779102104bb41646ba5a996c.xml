        <condition/>
        <description>This business rule resets the is_recent_factor column appropriately based on the logic that only one factor among all the factors setup by the user can only set as the recent used factor</description>
        <execute_function>false</execute_function>
        <filter_condition table="sys_user_multi_factor_setup">is_recent_factorCHANGESTOtrue^EQ<item endquery="false" field="is_recent_factor" goto="false" newquery="false" operator="CHANGESTO" or="false" value="true"/>
            <item endquery="true" field="" goto="false" newquery="false" operator="=" or="false" value=""/>
        </filter_condition>
        <is_rest>false</is_rest>
        <message/>
        <name>Mark Non Recent Used Factors</name>
        <order>200</order>
        <priority>100</priority>
        <rest_method/>
        <rest_method_text/>
        <rest_service/>
        <rest_service_text/>
        <rest_variables/>
        <role_conditions/>
        <script><![CDATA[(function executeRule(current, previous /*null when async*/ ) {
    try {
		SNC.MultifactorAuthUtil.markNonRecentFactor(current.user, current.factor_type);
    } catch (e) {
        gs.error("Set Most Recent Used Factor BR: error occured while setting the recent factor for user:" + current.user + ", error: " + e);
	}
})(current, previous);]]></script>
